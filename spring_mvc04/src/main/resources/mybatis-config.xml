<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
 	<!-- Mybatis에서 쿼리에 매핑되는 파라미터에 Null값이 있는 경우 에러 발생 => 해결 -->
 	<!-- Mybatis cashe 사용 여부  -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
		<setting name="cacheEnabled" value="false"/>
	</settings>
	<typeAliases>
		<typeAlias type="com.ict.model.vo.MembersVO" alias="mvo"/>
		<typeAlias type="com.ict.model.vo.GuestBookVO" alias="gvo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mappers/members-mapper.xml" /> <!-- my mapper -->
		<mapper resource="mappers/guest-mapper.xml" /> <!-- my mapper -->
	</mappers>
</configuration>

